{
    "name": "symfony/framework-standard-edition",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-0": { "": "src/", "Bpi": ["vendor/bpi-dk/bpi-client/Bpi/Sdk", "vendor/bpi-dk/bpi-client/"] }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "bpi-dk/bpi-client",
                "version": "1.0",
                "source": {
                    "type": "git",
                    "url": "git@github.com:easyting/bpi-client.git",
                    "reference": "origin/develop"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.1.10",
        "twig/twig": "v1.13.0",
        "doctrine/doctrine-bundle": "1.1.*",
        "doctrine/orm": "2.3.2",
        "doctrine/dbal": "2.3.4",
        "twig/extensions": "1.0.*",
        "symfony/assetic-bundle": "2.3.1",
        "symfony/monolog-bundle": "2.1.*",
        "sensio/distribution-bundle": "2.1.*",
        "sensio/framework-extra-bundle": "2.1.*",
        "sensio/generator-bundle": "2.1.*",
        "jms/di-extra-bundle": "1.3.*",
        "jms/serializer-bundle": "0.11.0",
        "nelmio/api-doc-bundle": "2.2.*@dev",
        "friendsofsymfony/rest-bundle": "0.11.0",
        "doctrine/mongodb": "^1.2",
        "doctrine/mongodb-odm-bundle": "3.0.*",
        "doctrine/doctrine-fixtures-bundle": "dev-master",
        "stof/doctrine-extensions-bundle": "dev-master",
        "knplabs/knp-gaufrette-bundle": "0.2.*@dev",
        "knplabs/knp-markdown-bundle": "1.2.*@dev",
        "knplabs/knp-paginator-bundle": "2.3.2",
        "antimattr/mongodb-migrations": "~1.0@stable",
        "antimattr/mongodb-migrations-bundle": "~1.0@stable",
        "symfony/swiftmailer-bridge": "2.0.10",
        "symfony/swiftmailer-bundle": "2.0.10"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "phpunit/phpunit-story": "*",
        "fabpot/goutte": "1.0.1",
        "bpi-dk/bpi-client": "1.0"
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web"
    }
}
